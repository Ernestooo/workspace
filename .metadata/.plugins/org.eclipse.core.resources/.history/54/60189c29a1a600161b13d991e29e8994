package abs;

public class main {

	public static void main(String[] args) {
		String filmSeries = "Tin Tin 201";
		 int inc = 0;
		 boolean cond = true;
		 filmSeries = filmSeries + inc++;
		 if (inc >= 1 || !cond){
			 inc--;
		 	filmSeries = filmSeries + inc;} 
		 System.out.println(filmSeries);
		 System.out.println(inc);
		 int a = 0;
		 a++;
		 System.out.println(a);
		 System.out.println("Ex. 2");
		 int x1 = 3;
		 int x2 = 5;
		 int x3 = 7;
		 	x1 = x1 % x2;
		 	x2 = x2 % x3;
		 	x3 = x3 % x1;
		 System.out.println(x1);
		 System.out.println(x2);
		 System.out.println(x3);
		 if (x1 > x3 && x2 < x1){
		 x1 = (x1 + x2) / x3;}
		 else if (x2 > x3){
		 x1 = x3 + x3; }
		 System.out.println(x1);
		 System.out.println(x2);
		 System.out.println(x3);
	}

}
